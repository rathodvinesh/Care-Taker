<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.ApplicationsVolunteerFragment">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/hamburger" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/inter_medium"
        android:gravity="center"
        android:text="Client List"
        android:textColor="@color/button_color"
        android:textSize="32sp"
        android:textStyle="bold" />

    <androidx.cardview.widget.CardView
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="120dp"
        android:visibility="visible"
        app:cardCornerRadius="5dp">

        <!-- Your item layout content here -->
        <TextView
            android:id="@+id/textView54"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="100dp"
            android:fontFamily="@font/inter_medium"
            android:text="Shift:"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="65dp"
            android:layout_height="20dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="170dp"
            android:fontFamily="@font/inter_medium"
            android:text="Amount:"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/amountTV"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:layout_marginStart="290dp"
            android:layout_marginTop="170dp"

            android:fontFamily="@font/inter_medium"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="105dp"
            android:layout_height="111dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/baseline_person_24" />

        <TextView
            android:id="@+id/serviceTV"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:layout_marginStart="290dp"
            android:layout_marginTop="140dp"

            android:fontFamily="@font/inter_medium"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/contactTV"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:layout_marginStart="195dp"
            android:layout_marginTop="100dp"

            android:fontFamily="@font/inter_medium"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/genderTV"
            android:layout_width="65dp"
            android:layout_height="25dp"
            android:layout_marginStart="300dp"
            android:layout_marginTop="60dp"

            android:fontFamily="@font/inter_medium"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/shiftTV"
            android:layout_width="45dp"
            android:layout_height="25dp"
            android:layout_marginStart="320dp"
            android:layout_marginTop="100dp"

            android:ems="10"
            android:fontFamily="@font/inter_medium"
            android:inputType="text"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="55dp"
            android:layout_height="20dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/inter_medium"
            android:text="Name:"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nameTV"
            android:layout_width="185dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="20dp"

            android:fontFamily="@font/inter_medium"
            android:textSize="14sp"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="40dp"
            android:layout_height="25dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="60dp"
            android:fontFamily="@font/inter_medium"
            android:text="Age:"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="65dp"
            android:layout_height="25dp"
            android:layout_marginStart="230dp"
            android:layout_marginTop="60dp"
            android:fontFamily="@font/inter_medium"
            android:text="Gender:"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="70dp"
            android:layout_height="20dp"
            android:layout_marginStart="120dp"
            android:layout_marginTop="100dp"
            android:fontFamily="@font/inter_medium"
            android:text="Address:"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="65dp"
            android:layout_height="20dp"
            android:layout_marginStart="220dp"
            android:layout_marginTop="140dp"
            android:fontFamily="@font/inter_medium"
            android:text="Service:"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/ageTV"
            android:layout_width="45dp"
            android:layout_height="25dp"
            android:layout_marginStart="170dp"
            android:layout_marginTop="60dp"

            android:fontFamily="@font/inter_medium"
            android:textSize="14sp"
            android:visibility="visible"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/hireBtn"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="140dp"
            android:backgroundTint="@color/button_color"
            android:text="View Profile"
            android:textColor="@color/white"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/profileBtn"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="110dp"
            android:layout_marginTop="140dp"
            android:backgroundTint="@color/button_color"
            android:fontFamily="@font/inter_medium"
            android:text="Accept"
            android:textColor="@color/white"
            android:textColorLink="#2196F3"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="380dp"
        android:layout_height="100dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="340dp">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="100dp"
            android:layout_marginTop="50dp"
            android:src="@drawable/baseline_check_24"
            android:tint="#219526"
            tools:ignore="UseAppTint" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/inter_medium"
            android:text="Name:"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="130dp"
            android:layout_marginTop="50dp"
            android:text="Request Accepted"
            android:textColor="#219526"
            android:textSize="20sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText2"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="160dp"
            android:layout_marginTop="10dp"
            android:ems="10"
            android:inputType="text"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="380dp"
        android:layout_height="100dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="450dp">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            tools:src="@tools:sample/avatars" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="100dp"
            android:layout_marginTop="50dp"
            android:src="@drawable/baseline_close_24"
            android:tint="#F44336"
            tools:ignore="UseAppTint" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="100dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/inter_medium"
            android:text="Name:"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="130dp"
            android:layout_marginTop="50dp"
            android:text="Request Declined"
            android:textColor="#F44336"
            android:textSize="20sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText3"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginStart="160dp"
            android:layout_marginTop="10dp"
            android:ems="10"
            android:inputType="text"
            android:textSize="16sp" />
    </androidx.cardview.widget.CardView>




</FrameLayout>